{
    "manifest_version": 2,
    "name": "Stanley Memer",
    "description": "Shields users from memes passing the dankness threshold",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["jquery-3.1.1.min.js","content.js"],
            "run_at": "document_end"
        }
    ],
    "permissions":[
        "*://*/"
    ],
    "web_accessible_resources": [
        "images/TooDank.jpg"
    ]
}